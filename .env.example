# ===========================================
# GEO/SEO Knowledge Base - Environment Config
# ===========================================

# OpenAI API Key (Required - for PDF analysis and embeddings)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o

# Google API Key (Optional - fallback for Gemini)
GOOGLE_API_KEY=

# Crawl4AI Configuration (for web crawling to find scholarly papers)
# No API key required - uses local browser automation

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_API_KEY=

# LangSmith (Optional - for monitoring)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=geo-seo-kb

# Processing Configuration (Optional - defaults in config.py)
MAX_CONCURRENT_DOCUMENTS=3
MAX_MEMORY_MB=2048
SIMILARITY_THRESHOLD=0.85
MAX_PDF_SIZE_MB=50
MAX_PDF_PAGES=500

# Deep Agent Configuration
DEEP_AGENT_MODEL=claude-sonnet-4-5-20250929
MAX_SEARCH_RESULTS=10
PDF_DOWNLOAD_DIR=./pdfs
